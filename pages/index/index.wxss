/* 头部图片 */
.box {
  width: 350px;
  height: 1030px;
  margin: 0 auto;
  margin-top: 10px;
}

.top-banner {
  width: 350px;
  height: 140px;
  border-radius: 30px;
  position: relative;
}

.bannerImage {
  position: absolute;
}

.banner-content {
  margin-left: 15px;
  width: 160px;
  height: 100px;
  position: absolute;
  z-index: 1;
  margin-top: 20px;
}

.go {
  font-weight: 500;
  color: white;
  width: 60px;
  height: 25px;
  font-size: 12px;
  border: 1px solid white;
  border-radius: 30px;
  text-align: center;
  line-height: 25px;
  margin-top: 10px;
}

.types {
  display: flex;
  width: 100%;
  height: 70px;
  margin-top: 10px;
  flex-direction: row;
  justify-content: space-around;

}

